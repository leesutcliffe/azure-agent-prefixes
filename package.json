{
  "name": "azure-agent-prefix-automation",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "nyc": {
    "all": true,
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/index.js"
    ],
